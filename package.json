{
  "name": "tree-sitter-purescript",
  "version": "0.0.1",
  "description": "purescript grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "tree-sitter": [
    {
      "scope": "source.purescript",
      "file-types": [ "purs" ],
      "first-line-regex": "#!.*\\bpurs$"
    }
  ],
  "scripts": {
    "test": "tree-sitter test"
  }
}
